<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ScorePage">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Score Page"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.193" />

    <TextView
        android:id="@+id/outputUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Username:"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/outputScore"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.353" />

    <TextView
        android:id="@+id/outputQuiz1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Quiz #1:"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/outputQuiz2"
        app:layout_constraintEnd_toEndOf="@+id/outputUsername"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="@+id/outputUsername"
        app:layout_constraintTop_toBottomOf="@+id/outputUsername"
        app:layout_constraintVertical_bias="0.772" />

    <TextView
        android:id="@+id/outputQuiz2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Quiz #2:"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/outputQuiz3"
        app:layout_constraintEnd_toEndOf="@+id/outputQuiz1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/outputQuiz1"
        app:layout_constraintTop_toBottomOf="@+id/outputUsername"
        app:layout_constraintVertical_bias="0.88" />

    <TextView
        android:id="@+id/outputQuiz3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Quiz #3:"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/outputScore"
        app:layout_constraintEnd_toEndOf="@+id/outputQuiz2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/outputQuiz2"
        app:layout_constraintTop_toBottomOf="@+id/outputUsername"
        app:layout_constraintVertical_bias="0.87" />

    <TextView
        android:id="@+id/outputScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Total Score:"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/outputUsername"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="@+id/outputUsername"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.629" />

</androidx.constraintlayout.widget.ConstraintLayout>